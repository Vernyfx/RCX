local Tab = Window:CreateTab("Auto Egg")

local eggs = game:GetService("Workspace").Scripts.Eggs:GetChildren()

local eggsTable = {}

for i,v in ipairs(eggs) do
    if v:IsA("Model") then
        table.insert(eggsTable, v.Name)
    end
end

local Dropdown = Tab:CreateDropdown({
	Name = "Select Egg",
	Options = eggsTable,
	CurrentOption = " ",
	Flag = "EggsDropdown", 
	Callback = function(EDValue)
        _G.selectegg = EDValue;
	end,
})

local Dropdown = Tab:CreateDropdown({
	Name = "Select Egg Hatches",
	Options = {"Single","Triple"},
	CurrentOption = " ",
	Flag = "EggsDropdownSelect",
	Callback = function(SEValue)
        _G.eggopen = SEValue;
	end,
})

local Toggle = Tab:CreateToggle({
	Name = "Auto Open Eggs",
	CurrentValue = false,
	Flag = "AutoOpenEggs", 
	Callback = function(AUEValue)
        _G.autoeggs = AUEValue;
	end,
})

task.spawn(function()
    while wait() do
        if _G.eggopen then
            if _G.autoeggs then
                if _G.selectegg ~= nil then
                    game:GetService("ReplicatedStorage").Functions.Unbox:InvokeServer(_G.selectegg,_G.eggopen)
                end
            end
        end
    end
end)

local Tab = Window:CreateTab("Chests")

local chests = game:GetService("Workspace").Scripts.Chests:GetDescendants()




local Toggle = Tab:CreateToggle({
	Name = "Auto Collect Chest",
	CurrentValue = false,
	Flag = "AutoChest",
	Callback = function(CHValue)
        _G.autochest = CHValue;
	end,
})

task.spawn(function()
    while wait() do
        if _G.autochest then
            wait(1)
            for i,v in ipairs(chests) do
                if v.Name == "TouchInterest" and v.Parent then
                    firetouchinterest(ph, v.Parent, 0)
                    wait(0.1)
                    firetouchinterest(ph, v.Parent, 1)
                end
            end
        end
    end
end)

