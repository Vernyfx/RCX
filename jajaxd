
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
	Name = "VernyHub",
	LoadingTitle = "Rebirth Champions X by VernyFX",
	LoadingSubtitle = "by Sirius",
	ConfigurationSaving = {
		Enabled = true,
		FileName = "Verny Hub, Rebirth Champs X"
	},
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Verny Hub",
		Subtitle = "Key System",
		Note = "Join the discord https://discord.gg/vEQDXpeX)",
		Key = "DoTheLinkvertise"
	}
})

local Tab = Window:CreateTab("Main")

local Toggle = Tab:CreateToggle({
	Name = "Auto Clicker",
	CurrentValue = false,
	Flag = "Auto Click",
	Callback = function(ACValue)
        _G.autoclick = ACValue;
	end,
})

task.spawn(function()
    while wait() do
        if _G.autoclick then
            game:GetService("ReplicatedStorage").Events.Click3:FireServer()
            wait()
        end
    end
end)

local rebsTable = {}

for i=1,80 do
    table.insert(rebsTable,i)
end

local Dropdown = Tab:CreateDropdown({
	Name = "Select Rebirth Option",
	Options = rebsTable,
	CurrentOption = " ",
	Flag = "RebirthDrop",
	Callback = function(RDValue)
        _G.rebdrop = RDValue;
	end,
})

local Toggle = Tab:CreateToggle({
	Name = "Auto Rebirth",
	CurrentValue = false,
	Flag = "RebirthToggle",
	Callback = function(ARValue)
        _G.autoreb = ARValue;
	end,
})

task.spawn(function()
    while wait() do
        if _G.autoreb then
            if _G.rebdrop ~= nil then
                game:GetService("ReplicatedStorage").Events.Rebirth:FireServer(_G.rebdrop)
            end
        end
    end
end)

local Button = Tab:CreateButton({
	Name = "Load Confing",
	Callback = function()
		Rayfield:LoadConfiguration()
	end,
})

local Tab = Window:CreateTab("Halloween!!!")

local pumpkins = game:GetService("Workspace").Scripts.PumpkinsCollect.SpawnPoints:GetChildren()

local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart

local storage = game:GetService("Workspace").Scripts.PumpkinsCollect.Storage:GetDescendants()

local ph = game.Players.LocalPlayer.Character.Head


local Toggle = Tab:CreateToggle({
	Name = "Auto Collect Pumpkins",
	CurrentValue = false,
	Flag = "AutoPumpkins",
	Callback = function(APPValue)
        _G.autopumpkins = APPValue;
	end,
})

local Button = Tab:CreateButton({
	Name = "Collect All Pumpkins",
	Callback = function()
        task.spawn(function()
            while wait() do
                         if _G.autopumpkins then
                                    for i,v in ipairs(storage) do
                                    if v.Name == "main" then
                                hrp.CFrame = v.CFrame
                            wait(0.1)
                        end
                    end
                end
            end
        end)
	end,
})

