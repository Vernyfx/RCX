local Tab = Window:CreateTab("World Teleport")


local worlds = game:GetService("Workspace").Scripts.TeleportTo:GetChildren()

local worldsTable = {}

for i,v in ipairs(worlds) do
    if v:IsA("Part") then
        table.insert(worldsTable, v.Name)
    end
end

local Dropdown = Tab:CreateDropdown({
	Name = "Select World",
	Options = worldsTable,
	CurrentOption = " ",
	Callback = function(WorldV)
        _G.world = WorldV;
	end,
})

local Button = Tab:CreateButton({
	Name = "Teleport To Selected World",
	Callback = function()
        hrp.CFrame = game:GetService("Workspace").Scripts.TeleportTo[_G.world].CFrame
	end,
})

local portals = game:GetService("Workspace").Scripts.Portals:GetDescendants()

local Button = Tab:CreateButton({
	Name = "Unlock All Maps",
	Callback = function()
                for i,v in ipairs(portals) do
            if v.Name == "Unlocked" then
                v.Value = true;
            end
        end
	end,
})

